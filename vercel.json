{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/users", "dest": "/api/users.js" },
    { "src": "/api/items", "dest": "/api/items.js" },
    { "src": "/api/favorites", "dest": "/api/favorites.js" },
    { "src": "/api/itenerary", "dest": "/api/itenerary.js" },
    { "src": "/api/pricing", "dest": "/api/pricing.js" }
  ]
}